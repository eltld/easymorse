{
  "name": "mldonkey Extension",
  "version": "1.0",
  "description": "mldonkey extension.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "http://*/*", "https://*/*"
  ],
    "content_scripts": [
    {
       "js": [ "jquery.js", "main.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at":"document_end"
    }
  ],
  "options_page": "options.html",
  "background_page": "background.html"
}